{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach Karma Chrome",
      "address": "127.0.0.1",
      "port": 9876,
      "pathMapping": {
        "/": "${workspaceRoot}/admin/",
        "/base/": "${workspaceRoot}/admin/"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Server",
      "program": "${workspaceFolder}/server/server.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Install",
      "program": "${workspaceFolder}/install.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Reset",
      "program": "${workspaceFolder}/server/install/npm-reset.js"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Admin Client",
      "url": "http://localhost:4200/admin/content/5d083cfedadbaea3879324fc",
      "webRoot": "${workspaceFolder}/admin"
    }
  ]
}
